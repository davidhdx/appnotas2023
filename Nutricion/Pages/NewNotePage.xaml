<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Nutricion.Pages.NewNotePage"
             Title="New Note">
    <VerticalStackLayout>
        <Entry x:Name="txtTitle" Placeholder="Title">
        </Entry>
        <Entry x:Name="txtText" Placeholder="Write...">
        </Entry>
        <Button
            x:Name="btnCreateNote"
            Clicked="btnCreateNote_Clicked">
        </Button>

    </VerticalStackLayout>
</ContentPage>